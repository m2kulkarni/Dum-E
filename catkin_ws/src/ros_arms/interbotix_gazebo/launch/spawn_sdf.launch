<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="camera_name"          default=""/>
    <arg name="x"                    default="0.0"/>
    <arg name="y"                    default="0.0"/>
    <arg name="z"                    default="1.0"/>
    <arg name="roll"                 default="0"/>
    <arg name="pitch"                default="0"/>
    <arg name="yaw"                  default="0.0"/>
    <arg name="sdf_camera_file"       default=""/>

    <node name="$(arg camera_name)_spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-file $(arg sdf_camera_file) -sdf -x $(arg x) -y $(arg y)  -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -model $(arg camera_name)" />
</launch>
